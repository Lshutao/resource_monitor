<form name="userDemoUpdateForm" action="/userDemo/save" method="POST" data-resultType="JSON">
	<table class="table table-bordered table-striped formLayout m0">
		<tr>
			<td>id:</td>
			<td><input type="text" name="id" value="$!{userDemo.id}" placeholder="id" readonly/></td>
			<td>userName:</td>
			<td><input type="text" name="userName" value="$!{userDemo.userName}" placeholder="userName"/></td>
		</tr>
		<tr>
			<td>password:</td>
			<td><input type="text" name="password" value="$!{userDemo.password}" placeholder="password"/></td>
			<td>createdTime:</td>
			<td><input type="text" name="createdTime" value="$!{userDemo.createdTime}" placeholder="createdTime"/></td>
		</tr>
		<tr>
			<td>updatedTime:</td>
			<td><input type="text" name="updatedTime" value="$!{userDemo.updatedTime}" placeholder="updatedTime"/></td>
			<td>deletedTime:</td>
			<td><input type="text" name="deletedTime" value="$!{userDemo.deletedTime}" placeholder="deletedTime"/></td>
		</tr>
		<tr>
			<td>deleted:</td>
			<td colspan="3"><input type="text" name="deleted" value="$!{userDemo.deleted}" placeholder="deleted"/></td>
		</tr>
	</table>
</form>
<div class="bottomBar">
	<button class="btn btn-primary" onclick="$('form[name=userDemoUpdateForm]').submit()">保存更改</button>
</div>